<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
    >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/dimen_ranking_race_rowsize"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/id_ranking_textview_race"
            android:layout_width="0px"
            android:layout_height="@dimen/dimen_ranking_race_rowsize"
            android:layout_weight="5"
            android:textColor="@color/bluesky"
            android:textSize="@dimen/dimen_detail_LITTLE"/>

        <TextView
            android:id="@+id/id_ranking_textview_swimtime"
            android:layout_width="0px"
            android:layout_height="@dimen/dimen_ranking_race_rowsize"
            android:layout_weight="2"
            android:textColor="@color/redstop"
            android:textSize="@dimen/dimen_detail_MEDIUM"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/id_ranking_layout_race_tofill"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="45dp">

    </LinearLayout>


</LinearLayout>